

<div style="text-align:center">
  <br><br>
<div><h1>Our Students</h1></div>

<% @houses.each do |house| %>
  <br><br>
  <h3><%= link_to house.name, house_path(house) %></h3>
  <% house.students.each do |student| %>
    <p><%= link_to student.name, student_path(student)%><p>
  <% end %>
<% end %>


<%= button_to "New Student", new_student_path, method: :get %>
</div>
